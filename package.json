{
  "name": "mapvue",
  "version": "0.3.0",
  "description": "MapVue is a comprehensive MapboxGL component library. You can easily and happily apply MapVue to your Vue projects. Import various data sources and layers in the form of components, and modify the state of the layer by modifying the parameters of props.",
  "author": {
    "name": "chinesejar",
    "email": "chinesejar@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/timeroute/mapvue.git"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/mapvue.umd.cjs",
  "module": "./dist/mapvue.js",
  "exports": {
    ".": {
      "import": "./dist/mapvue.js",
      "require": "./dist/mapvue.umd.cjs"
    }
  },
  "license": "MIT",
  "keywords": [
    "mapbox-gl",
    "mapbox",
    "vue",
    "typescript",
    "vue3",
    "map",
    "gis"
  ],
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview --port 4173",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "peerDependencies": {
    "mapbox-gl": ">=1.2.0",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@mapbox/mapbox-gl-sync-move": "^0.3.1",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/geojson": "^7946.0.10",
    "@types/mapbox-gl": "^2.7.10",
    "@types/node": "^16.18.9",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.2.6",
    "@vue/tsconfig": "^0.1.3",
    "c8": "^7.12.0",
    "chart.js": "^3.9.1",
    "codecov": "^3.8.3",
    "echarts": "^5.4.1",
    "eslint": "^8.29.0",
    "eslint-plugin-vue": "^9.8.0",
    "geojson": "^0.5.0",
    "happy-dom": "^6.0.4",
    "mapbox-gl": ">=1.2.0",
    "mapbox-gl-compare": "^0.4.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.1",
    "typescript": "~4.7.4",
    "vite": "^3.2.5",
    "vitest": "^0.21.1",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6",
    "vue-tsc": "^0.38.9"
  }
}