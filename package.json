{
  "name": "mapvue",
  "version": "0.3.3",
  "description": "MapVue is a comprehensive MapboxGL component library. You can easily and happily apply MapVue to your Vue projects. Import various data sources and layers in the form of components, and modify the state of the layer by modifying the parameters of props.",
  "author": {
    "name": "chinesejar",
    "email": "chinesejar@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/timeroute/mapvue.git"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/mapvue.umd.cjs",
  "module": "./dist/mapvue.js",
  "exports": {
    ".": {
      "import": "./dist/mapvue.js",
      "require": "./dist/mapvue.umd.cjs"
    }
  },
  "license": "MIT",
  "keywords": [
    "mapbox-gl",
    "mapbox",
    "vue",
    "typescript",
    "vue3",
    "map",
    "gis"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "build:examples": "vite build --config vite.examples.config.ts",
    "preview": "vite preview --port 4173",
    "preview:examples": "vite preview --port 4174 --config vite.examples.config.ts",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "peerDependencies": {
    "mapbox-gl": ">=1.2.0",
    "vue": ">=3.0"
  },
  "devDependencies": {
    "@mapbox/mapbox-gl-sync-move": "^0.3.1",
    "@rushstack/eslint-patch": "^1.10.2",
    "@types/geojson": "^7946.0.14",
    "@types/mapbox-gl": "^2.7.21",
    "@types/node": "^20.12.7",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/coverage-v8": "^1.5.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.4.5",
    "@vue/tsconfig": "^0.1.3",
    "chart.js": "^3.9.1",
    "echarts": "^5.5.0",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.25.0",
    "geojson": "^0.5.0",
    "happy-dom": "^6.0.4",
    "mapbox-gl": "^2.15.0",
    "mapbox-gl-compare": "^0.4.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "typescript": "~4.7.4",
    "vite": "^5.2.9",
    "vitest": "^1.5.0",
    "vue": "^3.4.22",
    "vue-router": "^4.3.0",
    "vue-tsc": "^2.0.6"
  }
}